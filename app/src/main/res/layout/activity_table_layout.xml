<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".TableLayout">

    <TableLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="16dp"
        android:stretchColumns="1"
        tools:ignore="MissingConstraints"/>

    <TableRow android:background="#128675"
        tools:ignore="MissingConstraints"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">

        <TextView
            android:layout_column="0"
            android:padding="3dip"
            android:text="Producto"
            android:textColor="@android:color/white"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />

        <TextView
            android:layout_column="2"
            android:padding="3dip"
            android:text="Subtotal"
            android:textColor="@android:color/white"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />
    </TableRow>


    <TableRow tools:ignore="MissingConstraints"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
        <TextView
            android:layout_column="0"
            android:padding="3dip"
            android:text="jabon de manos x1"
            tools:ignore="MissingConstraints"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />
    <TextView
        android:layout_column="2"
        android:gravity="left"
        android:padding="3dip"
        android:text="$2"
        tools:ignore="MissingConstraints"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />
</TableRow>



    <TableRow tools:ignore="MissingConstraints"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
    <TextView
        android:layout_column="0"
        android:padding="3dip"
        android:text="shampoo monster x1"
        tools:ignore="MissingConstraints"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />
    <TextView
        android:layout_column="2"
        android:gravity="left"
        android:padding="3dip"
        android:text="$10"
        tools:ignore="MissingConstraints"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />
</TableRow>


    <TableRow tools:ignore="MissingConstraints"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
    <TextView
        android:layout_column="0"
        android:padding="3dip"
        android:text="shampoo monster x1"
        tools:ignore="MissingConstraints"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />
    <TextView
        android:layout_column="2"
        android:gravity="left"
        android:padding="3dip"
        android:text="$10"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />
    </TableRow>

    <TableRow
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <TextView
            android:id="@+id/texView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_column="0"
            android:padding="3dip"
            android:text="Pastas Duria x2"
            tools:ignore="MissingConstraints" />
        <TextView
            android:id="@+id/textView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_column="2"
            android:gravity="left"
            android:padding="3dip"
            android:text="$18" />

    </TableRow>


    <TableRow
        android:layout_width="match_parent"
        android:layout_height="match_parent">
    <TextView
        android:id="@+id/texView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_column="0"
        android:padding="3dip"
        android:text="Detergente Limpiadin x1"
        tools:ignore="MissingConstraints" />
    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_column="2"
        android:gravity="left"
        android:padding="3dip"
        android:text="$13.4" />
    </TableRow>

    <View
        android:layout_height="2dip"
        android:background="#FF909090"
        tools:ignore="MissingConstraints"
        android:layout_width="match_parent"/>

    <TableRow tools:ignore="MissingConstraints"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
        <TextView
        android:layout_column="1"
        android:padding="3dip"
        android:text="Subtotal"
            android:textColor="#28675"
        tools:ignore="MissingConstraints"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />
        <TextView
            android:id="@+id/textView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_column="2"
            android:gravity="left"
            android:padding="3dip"
            android:text="$43.4" />
    </TableRow>



    <TableRow
        android:layout_width="match_parent"
        android:layout_height="match_parent">
    <TextView
        android:id="@+id/texView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_column="1"
        android:padding="3dip"
        android:text="Costo envio"
        android:textColor="#128675"
        tools:ignore="MissingConstraints" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_column="2"
        android:gravity="right"
        android:padding="3dip"
        android:text="$10"
        tools:ignore="MissingConstraints" />
    </TableRow>

    <TableRow
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <TextView
            android:id="@+id/texView8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_column="1"
            android:padding="3dip"
            android:text="Cupon"
            android:textColor="#128675"
            tools:ignore="MissingConstraints" />
        <TextView
            android:id="@+id/textView9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_column="2"
            android:layout_gravity="left"
            android:gravity="right"
            android:padding="3dip"
            android:text="$5"
            tools:ignore="MissingConstraints" />
    </TableRow>



    <TableRow
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <TextView
            android:id="@+id/texView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_column="1"
            android:padding="3dip"
            android:text="Total"
            android:textColor="#128675"
            tools:ignore="MissingConstraints" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_column="2"
            android:layout_gravity="left"
            android:gravity="right"
            android:padding="3dip"
            android:text="$48.4"
            tools:ignore="MissingConstraints" />
    </TableRow>




</androidx.constraintlayout.widget.ConstraintLayout>






